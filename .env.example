# Environment Configuration
ENVIRONMENT=dev
NAMESPACE=sas-viya

# Bastion Configuration
BASTION_HOST=bastion.example.com
BASTION_USER=ec2-user
BASTION_KEY_PATH=~/.ssh/bastion.pem

# Kubernetes Configuration
K8S_CLUSTER_NAME=sas-viya-cluster
K8S_API_ENDPOINT=https://k8s-api.internal.example.com:443
K8S_CONTEXT=sas-viya-context

# SAS Viya Configuration
SAS_VIYA_URL=https://sas-viya.example.com
SAS_VIYA_USERNAME=admin
SAS_VIYA_PASSWORD=changeme

# CAS Configuration
CAS_HOST=cas-controller.internal.example.com
CAS_PORT=5570

# Test Configuration
TEST_TIMEOUT=300
TEST_PARALLEL=false
TEST_RETRY_COUNT=3

# Report Configuration
REPORT_FORMAT=html
REPORT_OUTPUT_DIR=./reports
